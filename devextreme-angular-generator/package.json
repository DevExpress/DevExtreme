    "@types/mkdirp": "^0.5.2",
    "@types/node": "~8.10.66",
    "@types/yargs": "^6.6.0",
    "del": "^2.2.2",
    "gulp": "^4.0.2",
    "gulp-jasmine": "^2.4.2",
    "gulp-sourcemaps": "^2.6.5",
