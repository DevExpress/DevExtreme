{
    "env": {
        "node": true
    },
    "plugins": [
        "node"
    ],
    "extends": [
        "plugin:node/recommended"
    ],
    "rules": {
        "node/no-unpublished-require": "off",
        "node/no-unsupported-features/node-builtins": "off",
        "node/shebang": "off"
    },
    "globals": {
        "console": true
    }
}
