{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es6",
    "noImplicitAny": true,
    "allowJs": true,
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "sass": [
        "node_modules/sass-embedded"
      ],
    },
  },
}
