  "scripts": {
    "clean": "gulp clean",
    "lint": "npm-run-all -p -c lint-js lint-css lint-ts lint-renovation lint-dts",
    "lint-js": "eslint --ext .js .",
    "lint-ts": "eslint --ext .tsx,.ts --ignore-pattern /js/renovation/ --ignore-pattern /testing/renovation/ --ignore-pattern *.d.ts .",
    "lint-css": "stylelint scss",
    "lint-renovation": "eslint --ext .tsx,.ts ./js/renovation",
    "lint-dts": "eslint js --ext .d.ts --ignore-pattern /js/renovation/",
    "lint-staged": "lint-staged",
    "pre-commit": "node build/pre-commit/index.js",
    "build": "dotnet build build/build-dotnet.sln && cross-env BUILD_ESM_PACKAGE=true gulp default",
    "build:dev": "dotnet build build/build-dotnet.sln && cross-env DEVEXTREME_TEST_CI=TRUE BUILD_ESM_PACKAGE=true gulp default",
    "build-npm-devextreme": "dotnet build build/build-dotnet.sln && cross-env BUILD_ESM_PACKAGE=true SKIP_THEMEBUILDER=true gulp default",
    "build-dist": "dotnet build build/build-dotnet.sln && cross-env BUILD_ESM_PACKAGE=true gulp default --uglify",
    "build-themes": "gulp style-compiler-themes",
    "build:react": "gulp generate-react",
    "build:react:watch": "gulp generate-react-watch",
    "build:react:typescript": "gulp generate-react-typescript",
    "prepare-npm-renovation": "cd ./build/gulp/renovation-npm && npm i --no-audit --no-fund --no-package-lock",
    "build-npm-renovation:react": "npm run prepare-npm-renovation && gulp renovation-npm-react",
    "build-npm-renovation:react:production": "cross-env NPM_PRODUCTION=true npm run build-npm-renovation:react",
    "build:modular": "cd ./playground/modular && webpack",
    "build:modular:watch": "cd ./playground/modular && webpack --watch",
    "build:testing-playground": "gulp build-renovation-testing:watch --playgroundName",
    "compile:r": "node ./node_modules/typescript/bin/tsc -p ./renovation.tsconfig.json",
    "dev": "dotnet build build/build-dotnet.sln && cross-env DEVEXTREME_TEST_CI=true gulp dev",
    "dev-inprogress": "dotnet build build/build-dotnet.sln && cross-env DEVEXTREME_TEST_CI=true BUILD_INPROGRESS_RENOVATION=true gulp dev",
    "dev:watch": "cross-env DEVEXTREME_TEST_CI=true gulp dev-watch",
    "transpile-tests": "gulp transpile-tests",
    "discover-declarations": "dx-tools discover --js-scripts ./js --exclude js/renovation/",
    "update-ts": "dx-tools update-ts-bundle --js-scripts ./js --output-path ./ts/dx.all.d.ts --exclude js/renovation/",
    "validate-ts": "gulp validate-ts",
    "internal-tool": "dx-tools",
    "validate-declarations": "dx-tools validate-declarations --js-scripts ./js --exclude js/renovation/",
    "test-testcafe": "node ./testing/testcafe/runner",
    "test-testcafe-inprogress": "node ./testing/testcafe/runner --meta=renovation",
    "test-jest": "jest --coverage --maxWorkers=8",
    "test-jest:watch": "jest --watch",
    "test-styles": "npm run test-jest -- --config=./testing/styles/jest.config.json --coverage=false",
    "qunit-in-docker": "gulp qunit-in-docker --constel",
    "test-typescript": "gulp test-ts"
  },
  "browserslist": [
    "last 2 versions",
    "ios > 9",
    "> 1%"
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "node_modules"
    ]
  },
  "pre-commit": [
    "lint-staged",
    "pre-commit"
  ]
}
