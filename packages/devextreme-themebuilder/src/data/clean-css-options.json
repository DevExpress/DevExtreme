{
  "rebase": false,
  "format": {
    "breaks": {
      "afterAtRule": true,
      "afterBlockBegins": true,
      "afterBlockEnds": true,
      "afterComment": false,
      "afterProperty": true,
      "afterRuleBegins": true,
      "afterRuleEnds": true,
      "beforeBlockEnds": true,
      "betweenSelectors": true
    },
    "breakWith": "\n",
    "indentBy": 2,
    "indentWith": "space",
    "spaces": {
      "aroundSelectorRelation": true,
      "beforeBlockBegins": true,
      "beforeValue": true
    },
    "wrapAt": false,
    "semicolonAfterLastProperty": true
  },
  "level": 2
}
