{
    "name": "@devextreme/react",
    "sourceRoot": "packages/react/src",
    "projectType": "library",
    "generators": {},
    "targets": {
        "watch": {
            "executor": "nx:run-commands",
            "options": {
                "commands": [
                    "tsc -b -w ./tsconfig.package.json",
                    "rollup -c rollup/rollup.config.mjs --watch"
                ],
                "cwd": "packages/react",
                "parallel": true
            }
        }
    }
}
