{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "es7",
      "es2017.object",
      "dom"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "declaration": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitUseStrict": true,
    "noImplicitAny": false,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "checkJs": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "src/artifacts"
  ]
}
