{
    "esversion": 6,
    "eqeqeq": true,
    "eqnull": true,
    "expr": true,
    "freeze": true,
    "laxbreak": true,
    "noarg": true,
    "undef": true,
    "unused": "vars",
    "validthis": true,

    "browser": true,
    "devel": true,

    "-W069": true,
    "-W079": true,
    "predef": {
        "require": true,
        "module": true,
        "exports": true
    }
}
