{
  "name": "test_devextreme_modules",
  "version": "1.0.0",
  "scripts": {
    "build:webpack-cjs": "webpack --config ./webpack.config.cjs.js",
    "build:webpack-esm": "webpack --config ./webpack.config.js",
    "build:rollup-esm": "rollup -c",
    "build:parcel-esm": "parcel build ./src/modules_esm.js --no-cache --dist-dir ./dist/dist_parsel",
    "build:browserify-cjs": "cross-env NODE_PATH=./node_modules:./node_modules/globalize/dist browserify ./src/modules_cjs.js -o ./dist/dist_browserify/bundle_cjs.js",
    "build:vite-esm": "vite build"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "browserify": "^17.0.0",
    "cross-env": "^7.0.3",
    "devextreme": "file:devextreme-21.1.3-dev.tgz",
    "devextreme-cldr-data": "^1.0.3",
    "globalize": "^1.6.0",
    "parcel": "^2.0.0-beta.2",
    "rollup": "^2.50.0",
    "vite": "^2.3.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "alias": {
    "globalize$": "~/node_modules/globalize/dist/globalize.js",
    "globalize": "~/node_modules/globalize/dist/globalize",
    "cldr$": "~/node_modules/cldrjs/dist/cldr.js",
    "cldr": "~/node_modules/cldrjs/dist/cldr"
  }
}
