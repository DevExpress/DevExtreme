{
  "configurations": [
    {
      "name": "Debug",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "Build Style Compiler",
      "cwd": "${workspaceFolder}",
      "program": "bin/style-compiler.dll",
      "args": [
        "theme", "--version=18.2.0", "--output-path=./out/", "--theme-name=generic", "--color-scheme-name=light"
      ],
      "stopAtEntry": true,
      "console": "integratedTerminal"
    }
  ]
}
